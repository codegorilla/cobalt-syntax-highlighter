{
  "name": "cobalt-lang",
  "displayName": "Cobalt Lang",
  "description": "Syntax highlighting for cobalt lang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cobalt",
      "aliases": ["Cobalt", "cobalt"],
      "extensions": [".cb"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cobalt",
      "scopeName": "source.cobalt",
      "path": "./syntaxes/cobalt.tmLanguage.json"
    }]
  }
}
